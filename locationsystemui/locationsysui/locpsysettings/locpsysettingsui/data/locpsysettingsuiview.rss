RESOURCE CBA r_psyquery_softkeys_yes_yes_no
    {
    flags = 0;
    buttons = 
        {
        CBA_BUTTON 
            {
            id = EAknSoftkeyYes; 
            txt = text_softkey_yes;
            },
        CBA_BUTTON
            {
            id = EAknSoftkeyNo; 
